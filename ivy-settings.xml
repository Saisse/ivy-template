<?xml version="1.0" encoding="UTF-8" ?>
<ivysettings>
  <settings defaultResolver="chained"/>
  <resolvers>
    <chain name="chained" returnFirst="true">
      <ibiblio name="maven" m2compatible="true"/>
      <url name="scala-lang">
        <artifact
            pattern="http://scala-tools.org/repo-releases/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
      </url>
    </chain>
  </resolvers>
</ivysettings>
