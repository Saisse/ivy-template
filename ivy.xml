  <dependencies>
    <dependency org="org/scala-lang" name="scala-compiler" rev="2.7.2" conf="scala->default;compile->default" />
    <dependency org="org/scala-lang" name="scala-library" rev="2.7.2" conf="scala->default;compile->default" />
    <dependency org="org/specs" name="specs" rev="1.4.1" conf="test->default" />
    <dependency org="junit" name="junit" rev="4.5" conf="test->default"/>
  </dependencies>
</ivy-module>
